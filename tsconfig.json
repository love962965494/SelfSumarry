{
  "compilerOptions": {
      "target": "es5",
      "outDir": "dist",
      "module": "commonjs",
      "moduleResolution": "node",
      "skipDefaultLibCheck": true,
      "sourceMap": true,
      "declaration": true,
      "noUnusedLocals": true,
      "noUnusedParameters": false,
      "noFallthroughCasesInSwitch": true,
      "removeComments": true,
      "suppressImplicitAnyIndexErrors": true,
      "downlevelIteration": true,
      "lib": [ "es6", "dom" ]
  },
  "exclude": [
      "node_modules"
  ]
}